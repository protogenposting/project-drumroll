function number_to_difficulty_color(int){
	if(int==0)
	{
		return c_aqua
	}
	if(int==1)
	{
		return c_blue
	}
	if(int==2)
	{
		return c_lime
	}
	if(int==3)
	{
		return c_yellow
	}
	if(int==4)
	{
		return c_red
	}
	if(int==5)
	{
		return c_purple
	}
}